<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dell.eatit.MainActivity"
    android:background="@drawable/americanback2">


    <info.hoang8f.widget.FButton
        android:layout_width="match_parent"
        android:id="@+id/fbtnsigninmain"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:textColor="@color/colorwhite"
        android:text="SIGN IN"
        android:textSize="23sp"
        android:textStyle="bold"
        app:buttonColor="#32CD32"
        app:shadowEnabled="true"
        app:shadowHeight="5dp"
        app:cornerRadius="8dp"
        android:shadowColor="@color/colorblack"
        android:layout_marginBottom="27dp"
        android:layout_above="@+id/fbtnsignupmain"
        android:layout_alignParentEnd="true"></info.hoang8f.widget.FButton>
    <info.hoang8f.widget.FButton
        android:layout_width="match_parent"
        android:id="@+id/fbtnsignupmain"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:textColor="@color/colorwhite"
        android:text="SIGN UP"
        android:textSize="23sp"
        android:textStyle="bold"
        app:buttonColor="#006400"
        app:shadowEnabled="true"
        app:shadowHeight="5dp"
        app:cornerRadius="8dp"
        android:shadowColor="@color/colorblack"
        android:layout_marginBottom="19dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"></info.hoang8f.widget.FButton>
</RelativeLayout>
